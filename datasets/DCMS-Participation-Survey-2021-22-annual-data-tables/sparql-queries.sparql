PREFIX lower: <http://gss-data.org.uk/data/participation-survey/dcms-participation-survey-2021-22-annual-data-tables/participation-survey-by-highest-qualification#attribute/lower-confidence-interval>
SELECT DISTINCT ?survey ?response ?percentage ?upper ?lower
WHERE {
       ?obs qb:dataSet data: . 
       ?obs sr: [ rdfs:label ?survey] .
       ?obs res: [ rdfs:label ?response ] .	 
       ?obs measure: ?percentage .	
	   ?obs upper: ?upper .
	   ?obs lower: ?lower .
FILTER(?survey in ('Adults engagement with live sports in the last 6 months by demographics (physical)')) .	
      }       
ORDER BY ?response