{
    "@context": "http://www.w3.org/ns/csvw",
    "url": "ITL-Geo-Json.csv",
    "dialect": {
    	"delimiter": "|",
    	"headerRowCount": 1
  	},
    "rdfs:label": "ITL Geo JSON",
    "dc:title": "ITL Geo JSON",
    "tableSchema": {
        "columns": [
            {
                "titles": "Label",
                "name": "label",
                "datatype": "string",
                "required": true,
                "propertyUrl": "rdfs:label"
            },
            {
                "titles": "Notation",
                "name": "notation",
                "datatype": {
                    "base": "string",
                    "format": "^-?[\\w\\.\\/\\+]+(-[\\w\\.\\/\\+]+)*$"
                },
                "required": true,
                "propertyUrl": "skos:notation"
            },
            {
                "titles": "GeoJson",
                "name": "geojson",
                "datatype": "json",
                "required": true,
                "propertyUrl": "http://publishmydata.com/def/pmdgeo/asGeoJson"
            },
            {
                "titles": "Parent Notation",
                "name": "parent_notation",
                "datatype": {
                    "base": "string",
                    "format": "^(-?[\\w\\.\\/\\+]+(-[\\w\\.\\/\\+]+)*|)$"
                },
                "required": false,
                "propertyUrl": "skos:broader"
            },
            {
                "titles": "Sort Priority",
                "name": "sort_priority",
                "datatype": "integer",
                "required": false,
                "propertyUrl": "http://www.w3.org/ns/ui#sortPriority"
            }
        ],
        "primaryKey": "notation",
        "aboutUrl": "http://gss-data.org.uk/def/concept/itl-geo-json/{notation}"
    }
}